---
// This file now redirects to the /services/ version of the page
export const prerender = true;

import { getKeywordPaths } from '../../utils/seo';

export async function getStaticPaths() {
  return getKeywordPaths();
}

const { slug } = Astro.params;
---

<meta http-equiv="refresh" content={`0;url=/services/${slug}/`}>
<script>
  window.location.href = `/services/${window.location.pathname.split('/').pop()}/`;
</script> 